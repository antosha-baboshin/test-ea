{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "import/extensions": 0,
        "import/no-named-as-default": 0,
        "import/no-named-as-default-member": 0,
        "prefer-const": 0

    }
}
